@using ThuVien.Core.Models
@model IEnumerable<ThuVien.Core.Models.DonViDoanhNghiep>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<div>
    @Html.Partial("Create", new DonViDoanhNghiep())
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaDonVi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenDonVi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenTimKiem)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaDonVi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenDonVi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenTimKiem)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
